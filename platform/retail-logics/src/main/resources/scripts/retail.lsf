@defineAddDetailDialogSkuStockSupplier(returnSupplier, item, departmentStore, dialogSku);
@defineAddDetailDialogBarcode(returnSupplier, item);
@defineAddDetailDialogTerminal(returnSupplier, item);

@defineDocumentHeaderQuantity(returnSupplier);

