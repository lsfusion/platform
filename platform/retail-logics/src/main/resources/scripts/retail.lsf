        SET preliminaryOrderOrderSupplier(o) <- preliminaryOrder AS preliminaryOrder;
        SET nextDateOrderSupplier(o) <- nextDateOrder(preliminaryOrder);
        EXEC fillInOrderSupplier(o AS orderSupplier);
