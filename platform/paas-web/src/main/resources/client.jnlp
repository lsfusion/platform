<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.5+" codebase="${codebase.url}" href="${jnlp.url}">
    <information>
        <title>${scripted.name}</title>
        <vendor>Nomosoft Ltd.</vendor>
        <description>Fusion</description>
        <icon href="default.png" kind="default"/>
        <icon href="splash.jpg" kind="splash"/>
        <shortcut online="true">
            <desktop/>
        </shortcut>
    </information>
    <security>
        <all-permissions/>
    </security>
    <resources>
        <property name="jnlp.packEnabled" value="true"/>
        <j2se version="1.6+" initial-heap-size="32m" max-heap-size="128m"/>
        <property name="platform.client.hostname" value="${scripted.host}"/>
        <property name="platform.client.hostport" value="${scripted.port}"/>
        <property name="platform.client.exportname" value="${scripted.exportName}"/>
        <jar href="lsfusion-client-1.0-SNAPSHOT.jar"/>
    </resources>
    <application-desc/>
</jnlp>