MODULE MultiSelect;

REQUIRE SystemEvents;

NAMESPACE Utils;

onWebClientLoad() + {
    INTERNAL CLIENT 'selectize.js';
    INTERNAL CLIENT IF useBootstrap() THEN 'selectize.bootstrap5.css' ELSE 'selectize.css';
    IF NOT useBootstrap() THEN INTERNAL CLIENT 'select.css';
    INTERNAL CLIENT 'custom_selectize.js';
}