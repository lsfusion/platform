MODULE FrappeGantt;

REQUIRE SystemEvents;

NAMESPACE Utils;

// usage :
// CUSTOM 'gantt' OPTIONS JSON FROM view_mode = 'Day'

onWebClientInit() + {
    onWebClientInit('custom_frappe-gantt.css') <- 1;
    onWebClientInit('custom_frappe-gantt.js') <- 1;

    onWebClientInit('frappe-gantt.css') <- 1;
    onWebClientInit('frappe-gantt.js') <- 1;
}