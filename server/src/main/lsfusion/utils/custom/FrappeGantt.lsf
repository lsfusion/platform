MODULE FrappeGantt;

REQUIRE SystemEvents;

NAMESPACE Utils;

onWebClientLoad() + {
    onWebClientLoadData('custom_frappe-gantt.css') <- 1;
    onWebClientLoadData('custom_frappe-gantt.js') <- 1;
    
    onWebClientLoadData('frappe-gantt.css') <- 1;
    onWebClientLoadData('frappe-gantt.js') <- 1;
}